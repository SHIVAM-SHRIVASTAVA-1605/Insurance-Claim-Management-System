{
  "version": 2,
  "routes": [
    {
      "src": "^/$",
      "dest": "/website/index.html"
    },
    {
      "src": "^/app/?$",
      "dest": "/build/web/index.html"
    },
    {
      "src": "^/app/(.*)$",
      "dest": "/build/web/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/index.html"
    }
  ]
}