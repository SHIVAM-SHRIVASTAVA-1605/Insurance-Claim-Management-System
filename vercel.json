{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/website/index.html"
    },
    {
      "source": "/app",
      "destination": "/build/web/index.html"
    },
    {
      "source": "/app/(.*)",
      "destination": "/build/web/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/build/web/$1"
    }
  ]
}